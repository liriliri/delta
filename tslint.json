{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": {
            "options": [
                "single",
                "avoid-escape"
            ]
        },
        "space-before-function-paren": false,
        "one-line": false,
        "only-arrow-functions": false,
        "no-trailing-whitespace": false
    },
    "rulesDirectory": []
}